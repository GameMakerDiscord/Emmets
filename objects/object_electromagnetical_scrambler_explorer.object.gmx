        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!triggered){
    exit;
}
else{
    visible=true;
}
if (debugSpeed &gt; 0){
    timer += 1/60;
}

if ((debugSpeed&gt; 0 &amp;&amp; timer &gt;= debugSpeed) || debugSpeed &lt;= 0){

    timer = 0;
    
    var bx = round(x/G_CELLSIZE);
    var by = round(y/G_CELLSIZE);
    var thisChunk = onChunk(array(bx,by), object_ground_pathway);
    
    var missionMap = ds_map_find_value(object_game.electroMagneticalMissions, missionID);
    
    if (missionMap == undefined){
        missionMap = ds_map_logcreate();
        ds_map_add_list(missionMap, "doneList", ds_list_create());
        ds_map_add_map(object_game.electroMagneticalMissions, missionID, missionMap);
    }
    
    var doneList = ds_map_find_value(missionMap, "doneList");
    
    var coords = ds_list_create();
    
    //POWERING BUILDING
    var thisBuild = onChunk(array(bx,by), object_building_parent);
    
    if (instance_exists(thisBuild)){
        //show_debug_message(string(id)+" found building "+object_get_name(thisBuild.object_index)+" needing "+string(powerNeeded(thisBuild))+" power");
        warnTip(string_replace(translate("WARNING", "electroMagnetWarning"), "%s", translate(BUILDING, object_get_name(thisBuild.object_index))));
        thisBuild.powerSwitch = false;
    }
    
    //ENDOF
    
    ds_list_add(coords, array(bx, by+1), 
                        array(bx, by-1), 
                        array(bx-1, by), 
                        array(bx+1, by));
    
                        
    for (var j = 0; j &lt; 4; j++){
        var thisCoord = ds_list_find_value(coords, j);        
        var col = string(thisCoord[0])+","+string(thisCoord[1]);
        if (ds_list_find_index(doneList, col) == -1){
            
            var explorer = instance_create(thisCoord[0]*G_CELLSIZE, thisCoord[1]*G_CELLSIZE, object_electromagnetical_scrambler_explorer);
            explorer.debugSpeed= debugSpeed;
            explorer.missionID = missionID;
            explorer.visible = true;
            explorer.triggered = true;
            ds_list_add(doneList, string(thisCoord[0])+","+string(thisCoord[1]));
        }
    }
    
    ds_list_destroy(coords);
        
    ds_map_replace(missionMap, "doneList",doneList);
    
    //End of mission
    
    //If I am the last of my mission...
    count = 0;
    with (object_electromagnetical_scrambler_explorer){
        if (missionID == other.missionID){
            other.count++;
        }
    }
    
    if (count &lt;= 1 || instance_number(object_electromagnetical_scrambler_explorer) &gt; 128){        
        ds_list_destroy(doneList);
        ds_map_logdestroy(missionMap);
        ds_map_delete(object_game.electroMagneticalMissions, missionID);
        with (object_electromagnetical_scrambler_explorer){
            if (missionID == other.missionID){
                instance_destroy();
            }
        }
    }
    var col = make_color_hsv(missionID%255, 255, 255);
    spawnChunkPainter(getIsoIndex(array(x,y)), col, 0.3);
    instance_destroy();

}
</string>
