            <kind>1</kind>
            <string>/// COMPANY BAR display

depth = CURSOR_DEPTH+8;

if (isRoomAStation() &amp;&amp; object_station.companyLoaded &amp;&amp; 
    (   instance_exists(object_ui_window_eu) || 
        object_station.stationStep == STATION_MANAGE_CREW ||
        object_station.stationStep == STATION_CHOOSE_STOCK
    )
    ){

    var company = object_station.company;
    var margin = 2;

    var vx = view_xview;
    var vy = view_yview;
    var w = view_wview;
    var h = view_hview;
    
    draw_set_font(font_ui);
    draw_set_halign(fa_left);
    draw_set_valign(fa_bottom);
    
    var dateString = string(date_get_year(date_timestamp(company[? "creationTime"])));
    if (dateString != string(date_get_year(date_current_datetime()))){
        dateString += "-"+string(date_get_year(date_current_datetime()));
    }
    
    var leftText = company[? "name"] + " ©"+ dateString;
    var rightText = "₵"+numberFormat(company[? "funds"]);
    
    draw_set_color(COLOR_BACKGROUND());
    draw_rectangle(vx, vy + h - margin*2 - string_height(leftText) - UI_LEFT_BAND, vx+w-2, vy+h-2, false);
    
    draw_set_color(COLOR_SHADOW());
    draw_rectangle(vx+1, vy + h - margin*2 - string_height(leftText) - UI_LEFT_BAND+1, vx+w-1, vy+h-1, true);
    draw_set_color(COLOR_FOREGROUND());
    draw_rectangle(vx, vy + h - margin*2 - string_height(leftText) - UI_LEFT_BAND, vx+w-2, vy+h-2, true);
    
    drawShadowedText(vx + margin + UI_LEFT_BAND, vy + h - margin - UI_LEFT_BAND, leftText); 
    draw_set_halign(fa_right);
    var colors = array(COLOR_FOREGROUND(), COLOR_BACKGROUND(), COLOR_SHADOW());
    if (object_station.company[? "funds"] &lt;= 0){
        colors[0] = c_red;
    }
    drawShadowedText(vx + w - margin - UI_RIGHT_BAND, vy + h - margin - UI_LEFT_BAND, rightText, colors);

}
</string>
          </argument>
        </arguments>
      </action>
